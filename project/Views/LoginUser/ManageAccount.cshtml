@model IEnumerable<project.Models.AdminUser>

@{
    ViewBag.Title = "ManageAccount";
    Layout = "~/Views/Shared/_LayoutHome.cshtml";
}

<h2>ManageAccount</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NameUser)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RoleUser)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PasswordUser)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NameUser)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RoleUser)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PasswordUser)
        </td>
        <td>
            @Html.ActionLink("Delete", "DeleteAccount", new { id=item.ID })
        </td>
    </tr>
}

</table>
